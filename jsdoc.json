{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"], 
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {                      
    "destination": "./doc/",          
    "recurse": true,
    "encoding": "utf8",
    "readme":"./README.md"
  }
}